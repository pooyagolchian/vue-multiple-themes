{
  "name": "vue-multiple-themes",
  "version": "1.5.1",
  "description": "Vue Multiple Themes",
  "author": "Pooya Golchian(pooya.golchian@gmail.com)",
  "main": "dist/vue-multiple-themes.ssr.js",
  "browser": "dist/vue-multiple-themes.esm.js",
  "module": "dist/vue-multiple-themes.esm.js",
  "unpkg": "dist/vue-multiple-themes.min.js",
  "pub": "sh ./publish.sh",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "engines": {
    "node": ">=10"
  },
  "bugs": {
    "url": "https://github.com/pooyagolchian/vue-multiple-themes/issues"
  },
  "homepage": "https://github.com/pooyagolchian/vue-multiple-themes#readme",
  "keywords": [
    "vuejs-multiple-themes",
    "vue-dark-theme",
    "vue-server-theme-change",
    "vue-change-theme",
    "vue-dark-light-theme",
    "vue-multiple-color-pallet-themes"
  ],
  "license": "ISC",
  "repository": "pooyagolchian/vue-multiple-themes"
}
